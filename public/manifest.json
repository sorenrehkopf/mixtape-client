{
  "short_name": "Mixtape",
  "name": "Mixtape",
  "description": "Mixtape gives you fine-grain control over how you categorize and utilize your music.",
  "icons": [
    {
      "src": "/images/icons/android-icon-48x48.png",
      "type": "image/png",
      "sizes": "48x48"
    },
    {
      "src": "/images/icons/android-icon-72x72.png",
      "type": "image/png",
      "sizes": "72x72"
    },
    {
      "src": "/images/icons/android-icon-96x96.png",
      "type": "image/png",
      "sizes": "96x96"
    },
    {
      "src": "/images/icons/android-icon-144x144.png",
      "type": "image/png",
      "sizes": "144x144"
    },
    {
      "src": "/images/icons/android-icon-192x192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "/images/icons/android-icon-512x512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "start_url": "/",
  "background_color": "#382F35",
  "theme_color": "#382F34",
  "orientation": "portrait",
  "display": "standalone",
  "scope": "/",
  "serviceworker": {
    "src": "/service-worker.js",
    "scope": "/",
    "update_via_cache": "none"
  },
  "share_target": {
    "action": "/addSong",
    "params": {
      "text": "songId"
    }
  }
}